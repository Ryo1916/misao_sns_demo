<!DOCTYPE html>
<html>
  <head>
    <title>SnsApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': true %>
    <%= favicon_link_tag asset_path('favicon.png'), :rel => 'icon', :type => 'image/png' %>

    <!-- Compiled and minified CSS -->
    <!-- <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css"> -->

    <!-- Compiled and minified JavaScript -->
    <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script> -->
    <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.13.0/esm/popper-utils.js"></script> -->

    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyCtEA9jYRWka4cmSQ-XkZS9u02UWMWgSsI&callback=initMap", async: true, defer: true %>
    <!-- <script src="//maps.google.com/maps/api/js?key=AIzaSyBQpa8tTlVohI5Ha5bNv52DltGKePNckRc"></script> -->
    <!-- <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script> -->
    <!-- <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> -->
    <!-- <script async defer
        src="https://maps.googleapis.com/maps/api/js?v=3.exp&key=AIzaSyCtEA9jYRWka4cmSQ-XkZS9u02UWMWgSsI&callback=initMap">
    </script> -->
  </head>

  <body>
    <%= render "layouts/header" %>
    <% unless notice.blank? %>
      <p class="notice alert alert-success" role="alert" style="text-align:center; margin-bottom:-20px;"><%= notice %></p>
    <% end %>
    <% unless alert.blank? %>
      <p class="alert alert-danger" role="alert" style="text-align:center; margin-bottom:-20px;"><%= alert %></p>
    <% end %>

    <!-- <% if flash[:notice] %>
      <div class="flash">
        <%= flash[:notice] %>
      </div>
    <% end %> -->

    <%= yield %>

    <%= render "layouts/footer" %>
  </body>
</html>
